// a simple program in watchdog that prints "Hello, World!"

// call the standard library
call extern std;

// use vga to be able to print output
use std::vga;

// create a thread
spawn! id = 2, func = Hello;
{
	println("Hello, World!");
	
	// or
	// print("Hello, World!");
}

// main function
main
{
	Hello;
}
